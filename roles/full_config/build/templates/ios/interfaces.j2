{% for interface in devices[0]["interfaces"] %}
{% if 'Loop' in interface["name"] %}
{% include "./ios/interfaces/_loopback.j2" %}
{% elif 'Vlan' in interface["name"] %}
{% include "./ios/interfaces/_vlan.j2" %}
{% elif 'Port-Channel' in interface["name"] %}
{% include "./ios/interfaces/_portchannel.j2"%}
{% elif devices[0]["device_type"]["slug"] == "access_ios" %}
{% include "./ios/interfaces/_switch_l2_physical.j2"%}
{% elif devices[0]["device_type"]["slug"] == "router_ios" %}
{% include "./ios/interfaces/_router_physical.j2" %}
{% endif %}
{% endfor %}
