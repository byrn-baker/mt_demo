{% for vlan in devices[0]["site"]["vlans"]%}
   {% if vlan["vlan_vni"] != None %}
interface Vxlan1
   vxlan source-interface Loopback1
   vxlan udp-port 4789
   vxlan vlan {{vlan["vid"]}} vni {{vlan["vlan_vni"]}}
   {% endif %}
{% endfor%}
{% for vrf in vrfs %}
   {% if vrf["tenant"] != None %}
      {% if tenant in vrf["tenant"]["slug"] %}
   vxlan vrf {{ vrf["name"] }} vni {{ vrf["vrf_vni"] }}
      {% endif %}
   {% endif %}
{%endfor %}
!